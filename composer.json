{
    "name": "gustavocaiano/lakeclient",
    "description": "Laravel package to interact with the Lakeclient API",
    "keywords": [
        "licence",
        "api",
        "laravel"
    ],
    "homepage": "https://github.com/gustavocaiano/lakeclient",
    "license": "MIT",
    "authors": [
        {
            "name": "Gustavo Caiano",
            "email": "gustavocaiano@outlook.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^11.0||^12.0",
        "guzzlehttp/guzzle": "^7.7",
        "filament/filament": "^4.1"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "larastan/larastan": "^3.0",
        "orchestra/testbench": "^10.0.0||^9.0.0",
        "pestphp/pest": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "GustavoCaiano\\Lakeclient\\": "src/",
            "GustavoCaiano\\Lakeclient\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GustavoCaiano\\Lakeclient\\LakeclientServiceProvider"
            ],
            "aliases": {
                "Lakeclient": "GustavoCaiano\\Lakeclient\\Facades\\Lakeclient"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
